# This file was generated by ansible.
# Below is a list of all configured - remote - hosts in the stiltcluster group.

{% for item in stiltweb_remote_hosts %}
Host {{ item.host }}
  ServerAliveInterval=60
  User stiltcluster
  HostName {{ item.ip | default(item.host)}}
  RemoteForward {{ stiltweb_port }} localhost:{{ stiltweb_port }}
  LocalForward {{ item.port }} localhost:{{ item.port }}
  
{% endfor %}
